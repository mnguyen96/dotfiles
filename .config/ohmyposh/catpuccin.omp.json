{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#8AADF4",
    "lavender": "#B7BDF8",
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "text": "#494D64",
    "wight": "#FFFFFF"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "template": "{{.Icon}} ",
          "foreground": "p:wight",
          "powerline_symbol": "\ue0b4",
          "background": "p:blue",
          "type": "os",
          "style": "diamond"
        },
        {
          "template": "{{ .UserName }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:blue",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "full"
          },
          "template": " {{ .Path }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:pink",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": " {{ .HEAD }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:lavender",
          "type": "git",
          "style": "powerline"
        }
      ]
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#000000",
          "background": "#ebcc34",
          "template": " {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "properties": {
            "context_aliases": {
              "arn:aws-us-gov:eks:us-gov-west-1:415473649360:cluster/dev-precert": "dev-precert",
              "arn:aws-us-gov:eks:us-gov-west-1:415473649360:cluster/qa-precert": "qa-precert",
              "arn:aws-us-gov:eks:us-gov-west-1:415473649360:cluster/stage-precert": "stage-precert",
              "arn:aws-us-gov:eks:us-gov-west-1:415473649360:cluster/prod-precert": "prod-precert"
            }
          }
        },
        {
          "type": "terraform",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#81ff91",
          "template": "<#cc7eda> \u007C </><#7eb8da>\ue71e </> {{.WorkspaceName}}"
        },

        {
          "foreground": "#81ff91",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "<#cc7eda> \u007C </><#7eb8da>\ue718</> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "type": "node"
        },
        {
          "type": "sysinfo",
          "style": "powerline",
          "foreground": "#81ff91",
          "template": "<> </>"
        },
        {
          "background": "#cecece",
          "foreground": "#4b4b4b",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "style": "austin",
            "threshold": 150
          },
          "style": "diamond",
          "template": "⌛  {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#cd5e42",
          "style": "plain",
          "template": "\ue3bf ",
          "type": "root"
        },
        {
          "foreground": "#CD4277",
          "style": "plain",
          "template": "<#F5BDE6>\u276f</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "<#F5BDE6>\u276f </>"
  },
  "version": 3,
  "final_space": true
}
